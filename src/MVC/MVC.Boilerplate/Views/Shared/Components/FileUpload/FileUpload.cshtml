@model string
<div>
    <div class="form-group">
        @*<label asp-for="@Model" class="control-label"></label>*@
        <div class="custom-file">
            <input asp-for="@Model" type="file" name="@Model" class="custom-file-input" id="FL" accept="image/*,.pdf,.csv,.xls,.xlsx" onchange="return fileValidation();" style="display: none" />
            @*<label class="custom-file-label" for="FL">Choose File</label>*@
            <label id="btnSubmit" class="btn btn-large" value="Upload"><i class="fa fa-upload"></i></label> 
            <span id="spnFilePath"></span>
            <p id ="spnFileName" class="text-danger"></p>
        </div>
        @*<div><a href="" id="FileUrl"><h6></h6></a> <span><i class="btn btn-danger fa fa-trash"></i></span></div>*@


        <div class="row">
            <div class="col-md-9"> @Html.ActionLink($"{@ViewData["File"]}", "DownloadFile", new { fileName = @ViewData["FileUploadName"] }, new {style="text-decoration:none;"})</div>
            <div class="col-md-3">
                @if (@ViewData["FileUploadName"] != null)
                {
                    <a href="@Url.Action("Delete",  new { fileName = @ViewData["FileUploadName"] })">
                        <span><i class="btn btn-danger fa fa-trash"></i></span>
                    </a>
                }
            </div>
        </div>
        @*<div>@Html.ActionLink("", "Delete", new { fileName = @ViewData["FileUploadName"] })<span><i class="btn btn-danger fa fa-trash"></i></span></div>*@
 </div>
 </div>


    <script src="~/js/FileUpload.js" type="text/javascript" on-content-loaded="true" asp-append-version="true"></script>
    